doctype html
html

<head>
		<meta charset="utf-8">
		<title>#{title}</title>
		<link rel="stylesheet" href="/stylesheets/styles.css">
		<script type="text/javascript" src="/javascripts/jquery-3.6.0.min.js"></script>
</head>
		<body>
			<div class="wrapper">
				<header>
					
					
					<div class="heading">
						<h1 id="headname">#{pname}</h1>
					</div>
				</header>
				<nav>
					<ul class="top-menu">
						each item in navmenu
							<li><a href="#{item.addr}">#{item.name}</a></li>
					</ul>
				</nav>
				<main id="#{pname}">
					block content
				</main>
			</div>
			
<script type="text/javascript" src="/javascripts/topmenu.js"></script>
<script type="text/javascript" src="/javascripts/jqfunc.js"></script>
		</body>
